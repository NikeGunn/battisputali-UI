{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true, 
      "distribution": "internal",
      "android": {
        "buildType": "apk"     
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",  
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"    
    },
    "production": {
      "distribution": "store",   
      "android": {
        "buildType": "apk"        
      },
      "channel": "production"     
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
